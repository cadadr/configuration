[com/gkayaalp/pomodorino]
desktop-notifications-enabled = true
ease-in-mode-enabled = true
long-break-duration = 15
pomodoro-duration = 25
short-break-duration = 5

[org/gnome/cheese]
burst-delay = 1000
camera = 'Dummy video device (0x0000)'
photo-x-resolution = 720
photo-y-resolution = 1280
video-x-resolution = 720
video-y-resolution = 1280

[org/gnome/simple-scan]
disable-connected-notifications = false
disable-disconnected-notifications = false
disable-vpn-notifications = false
document-type = 'photo'
suppress-wireless-networks-available = false

[org/gtk/settings/file-chooser]
date-format = 'regular'
show-hidden = false
show-size-column = true
sort-directories-first = false

