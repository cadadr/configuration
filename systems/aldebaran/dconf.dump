[com/linuxmint/updates]
default-repo-is-ok = true
show-welcome-page = false

[org/cinnamon]
app-menu-label = 'Menu'
enabled-applets = ['panel1:left:0:menu@cinnamon.org:14', 'panel1:left:1:show-desktop@cinnamon.org:15', 'panel1:left:2:panel-launchers@cinnamon.org:16', 'panel1:left:3:window-list@cinnamon.org:17', 'panel1:right:0:systray@cinnamon.org:18', 'panel1:right:1:xapp-status@cinnamon.org:19', 'panel1:right:2:keyboard@cinnamon.org:20', 'panel1:right:3:notifications@cinnamon.org:21', 'panel1:right:4:printers@cinnamon.org:22', 'panel1:right:5:removable-drives@cinnamon.org:23', 'panel1:right:6:favorites@cinnamon.org:24', 'panel1:right:7:user@cinnamon.org:25', 'panel1:right:8:network@cinnamon.org:26', 'panel1:right:9:sound@cinnamon.org:27', 'panel1:right:10:power@cinnamon.org:28', 'panel1:right:11:calendar@cinnamon.org:29']
next-applet-id = 30
panel-launchers = ['DEPRECATED']
panel-zone-icon-sizes = '[{"panelId": 1, "left": 0, "center": 0, "right": 0}]'
panels-enabled = ['1:0:bottom']
panels-height = ['1:27']

[org/cinnamon/desktop/interface]
gtk-theme = 'Mint-Y-Dark-Purple'
icon-theme = 'Mint-Y-Dark-Purple'
scaling-factor = uint32 0

[org/cinnamon/desktop/sound]
event-sounds = false

[org/cinnamon/desktop/wm/preferences]
theme = 'Mint-Y-Dark'

[org/cinnamon/settings-daemon/plugins/power]
lid-close-ac-action = 'suspend'
lid-close-battery-action = 'suspend'
sleep-display-ac = 1800
sleep-display-battery = 1800
sleep-inactive-ac-timeout = 0
sleep-inactive-battery-timeout = 0

[org/cinnamon/theme]
name = 'Mint-Y-Dark-Purple'

[org/gnome/cheese]
burst-delay = 1000
camera = 'Integrated Camera: Integrated C'
photo-x-resolution = 800
photo-y-resolution = 448
video-x-resolution = 800
video-y-resolution = 448

[org/gnome/desktop/a11y/applications]
screen-keyboard-enabled = false
screen-reader-enabled = false

[org/gnome/desktop/a11y/mouse]
dwell-click-enabled = false
dwell-threshold = 10
dwell-time = 1.2
secondary-click-enabled = false
secondary-click-time = 1.2

[org/gnome/desktop/interface]
toolkit-accessibility = false

[org/gnome/evolution-data-server]
migrated = true
network-monitor-gio-name = ''

[org/nemo/window-state]
sidebar-bookmark-breakpoint = 5
start-with-sidebar = true

[org/x/editor/plugins]
active-plugins = ['open-uri-context-menu', 'textsize', 'time', 'docinfo', 'spell', 'sort', 'bracketcompletion', 'joinlines', 'filebrowser', 'modelines']

[org/x/editor/preferences/ui]
statusbar-visible = true

