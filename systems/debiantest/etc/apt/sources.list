# sources.list --- Debian unstable

# stable
deb http://ftp.uk.debian.org/debian/ stable main contrib non-free
deb-src http://ftp.uk.debian.org/debian/ stable main contrib non-free

# security
deb http://security.debian.org/debian-security stable/updates main
deb-src http://security.debian.org/debian-security stable/updates main

# updates
deb http://deb.debian.org/debian/ stable-updates main
deb-src http://deb.debian.org/debian/ stable-updates main

# stable debug syms
deb http://debug.mirrors.debian.org/debian-debug/ stable-debug main

# virtualbox
# deb [arch=amd64] https://download.virtualbox.org/virtualbox/debian buster contrib
