# msmtprc --- Send mail.

# Set default values for all following accounts.
defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
tls_starttls off
protocol smtp
# Leave â†“ blank for check netrc
password

# self@
account self
host __MAIL_HOST__
port 465
from __MAIL_ADDR__
user __MAIL_ADDR__
passwordeval "pass show email/__MAIL_ADDR__"

# Set a default account
account default : self
