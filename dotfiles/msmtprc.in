# msmtprc --- Send mail.

# Set default values for all following accounts.
defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
tls_starttls off
logfile ~/log/msmtp.log
protocol smtp
# Leave â†“ blank for check netrc
password

# self@
account self
host __MAIL_HOST__
port 465
from __MAIL_ADDR__
user __MAIL_ADDR__
passwordeval "pass show __MAIL_ADDR__"

# IU ogrenci mail
account iumail
host __IU_SMTP_HOST__
port 465
from __IU_MAIL_ADDR__
user __IU_MAIL_ADDR__
passwordeval "pass show __IU_MAIL_ADDR__"

# Set a default account
account default : self
